(function(){ myApp.controller("IndexController",["$scope","IndexFactory",function(a,IndexFactory){
a.message="Hello"
a.factoryValue=IndexFactory
a.myMethod=function(val){
alert("invoked" + val);
}
}]);

myApp.factory("IndexFactory",function(){
var mydata="this is mydata factory";	
	return{
 mydata2:"this is mydata factory",	
                getdata:function(){
 			return mydata;		
		}
	}
});
   }   )();





