alert("accordian");

myApp.controller('AccordionDemoCtrl', function ($scope) {
   $scope.groups = [
    {
      title: "Dynamic Group Header - 123",
      content: "Dynamic Group Body - 145",
      open: false
    },
    {
      title: "Dynamic Group Header - 2",
      content: "Dynamic Group Body - 2",
      open: false
    }
  ];
  
  $scope.addNew = function() {
    $scope.groups.push({
      title: "New One Created",
      content: "Dynamically added new one",
      open: false
    });
  }
  
});
